                            <td>{{correctingIns.OriginalInstitutionModel.Outpatients}}</td>
                        </tr>
                        <tr>
                            <td>所属总院</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Parent.Name!=correctingIns.OriginalInstitutionModel.Parent.Name]">{{correctingIns.InstitutionModel.Parent.Name}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Parent.Name}}</td>
                        </tr>
                        <tr>
                            <td>总院省份区县</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Parent.LocationName!=correctingIns.OriginalInstitutionModel.Parent.LocationName]">{{correctingIns.InstitutionModel.Parent.LocationName}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Parent.LocationName}}</td>
                        </tr>
                    </tbody>
                </table>

