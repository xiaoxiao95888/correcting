
@{
    Layout = null;
}
<h2>Admin</h2>

<div class="row">
    <table class="table table-responsive table-bordered table-hover">
        <thead>
            <tr>
                <th>提交编号</th>
                <th>员工号</th>
                <th>人员</th>
                <th>医院名称</th>               
                <th>是否新增</th>
                <th>提交日期</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="item in List" ng-class="{true:'active'}[item.TaskCode%2==0]">
                <td>{{item.TaskCode}}</td>
                <td>{{item.EmployeeModel.Code}}</td>
                <td>{{item.EmployeeModel.Name}}</td>
                <td>
                    <a href="#" ng-click="detail(item)">
                        {{item.InstitutionModel.Name}}
                    </a>
                </td>               
                <td>{{item.WhetherToAdd?'是':'否'}}</td>
                <td>{{item.DateTime | date:'yyyy-MM-dd HH:mm:ss'}}</td>
            </tr>
        </tbody>
    </table>   
</div>
<div class="row text-center">
    <a href="#" ng-click="load()" class="btn btn-success">刷新数据</a>
</div>
<div class="modal fade" id="detaildialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">{{correctingIns.InstitutionModel.Name}}</h4>
            </div>
            <div class="modal-body">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>字段</th>
                            <th>值</th>
                            <th>原始值</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>单位名称</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Name!=correctingIns.OriginalInstitutionModel.Name]">{{correctingIns.InstitutionModel.Name}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Name}}</td>
                        </tr>
                        <tr>
                            <td>电话号码</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.TelNum!=correctingIns.OriginalInstitutionModel.TelNum]">{{correctingIns.InstitutionModel.TelNum}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.TelNum}}</td>
                        </tr>
                        <tr>
                            <td>省份区县编号</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.LocationCode!=correctingIns.OriginalInstitutionModel.LocationCode]">{{correctingIns.InstitutionModel.LocationCode}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.LocationCode}}</td>
                        </tr>
                        <tr>
                            <td>省份区县</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.LocationName!=correctingIns.OriginalInstitutionModel.LocationName]">{{correctingIns.InstitutionModel.LocationName}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.LocationName}}</td>
                        </tr>
                        <tr>
                            <td>通讯地址</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Address!=correctingIns.OriginalInstitutionModel.Address]">{{correctingIns.InstitutionModel.Address}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Address}}</td>
                        </tr>
                        <tr>
                            <td>医院等次</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.LevelName!=correctingIns.OriginalInstitutionModel.LevelName]">{{correctingIns.InstitutionModel.LevelName}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.LevelName}}</td>
                        </tr>
                        <tr>
                            <td>医院级别</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.TierName!=correctingIns.OriginalInstitutionModel.TierName]">{{correctingIns.InstitutionModel.TierName}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.TierName}}</td>
                        </tr>
                        <tr>
                            <td>医院性质</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Nature!=correctingIns.OriginalInstitutionModel.Nature]">{{correctingIns.InstitutionModel.Nature}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Nature}}</td>
                        </tr>
                        <tr>
                            <td>医院属性</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Attribute!=correctingIns.OriginalInstitutionModel.Attribute]">{{correctingIns.InstitutionModel.Attribute}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Attribute}}</td>
                        </tr>
                        <tr>
                            <td>机构类型</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.InstitutionType!=correctingIns.OriginalInstitutionModel.InstitutionType]">{{correctingIns.InstitutionModel.InstitutionType}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.InstitutionType}}</td>
                        </tr>
                        <tr>
                            <td>专科方向</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.SpecializedDepartment!=correctingIns.OriginalInstitutionModel.SpecializedDepartment]">{{correctingIns.InstitutionModel.SpecializedDepartment}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.SpecializedDepartment}}</td>
                        </tr>
                        <tr>
                            <td>总床位数</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Beds!=correctingIns.OriginalInstitutionModel.Beds]">{{correctingIns.InstitutionModel.Beds}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Beds}}</td>
                        </tr>
                        <tr>
                            <td>2015年总门诊人数</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Outpatients!=correctingIns.OriginalInstitutionModel.Outpatients]">{{correctingIns.InstitutionModel.Outpatients}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Outpatients}}</td>
                        </tr>
                        <tr>
                            <td>所属总院</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Parent.Name!=correctingIns.OriginalInstitutionModel.Parent.Name]">{{correctingIns.InstitutionModel.Parent.Name}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Parent.Name}}</td>
                        </tr>
                        <tr>
                            <td>总院省份区县</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.Parent.LocationName!=correctingIns.OriginalInstitutionModel.Parent.LocationName]">{{correctingIns.InstitutionModel.Parent.LocationName}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.Parent.LocationName}}</td>
                        </tr>
                        <tr ng-show="correctingIns.InstitutionModel.ChildrenNames.length!=0">
                            <td>下属分院</td>
                            <td ng-class="{true:'text-danger'}[correctingIns.InstitutionModel.ChildrenNames!=correctingIns.OriginalInstitutionModel.ChildrenNames]">{{correctingIns.InstitutionModel.ChildrenNames}}</td>
                            <td>{{correctingIns.OriginalInstitutionModel.ChildrenNames}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" ng-click="delete()">删除</button>
                <button type="button" class="btn btn-primary" ng-click="pass()">通过</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
